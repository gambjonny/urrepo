{
  "name": "@urrepo/eslint-config",
  "type": "module",
  "version": "0.0.1",
  "description": "",
  "main": "eslint.config.js",
  "types": "index.d.ts",
  "exports": {
    "base": "./eslint.config.js",
    "import": "./eslint.config.js",
    "default": "./eslint.config.js"
  },
  "scripts": {
    "format:check": "wireit",
    "format:fix": "wireit"
  },
  "keywords": [],
  "engines": {
    "node": "22.12.0",
    "pnpm": "10.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "peerDependencies": {
    "eslint": "catalog:",
    "@eslint/js": "catalog:",
    "@vitest/eslint-plugin": "catalog:",
    "prettier": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "typescript": "catalog:"
  },
  "wireit": {
    "format:check": {
      "command": "prettier --ignore-path ../../.prettierignore --check .",
      "files": [
        "eslint.config.js"
      ]
    },
    "format:fix": {
      "command": "prettier --ignore-path ../../.prettierignore --write .",
      "files": [
        "eslint.config.js"
      ]
    }
  }
}
