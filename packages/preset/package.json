{
  "name": "@urrepo/preset",
  "type": "module",
  "version": "0.0.1",
  "description": "core preset",
  "main": "src/index.ts",
  "keywords": [
    "panda-css",
    "preset",
    "theme",
    "styling"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --ignore-path ../../.prettierignore --check .",
    "format:fix": "prettier --ignore-path ../../.prettierignore --write ."
  },
  "license": "ISC",
  "engines": {
    "node": "22.12.0",
    "pnpm": "10.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "dependencies": {
    "@urrepo/eslint-config": "workspace:*"
  },
  "peerDependencies": {
    "@pandacss/dev": "catalog:",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "eslint": "catalog:",
    "@eslint/js": "catalog:",
    "prettier": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "vitest": "catalog:",
    "@vitest/ui": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/eslint-plugin": "catalog:"
  }
}
