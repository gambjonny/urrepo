{
  "name": "@urrepo/root",
  "version": "0.0.1",
  "description": "a basic monorepo setup",
  "scripts": {
    "build:all": "tsc -b",
    "test:all": "vitest run",
    "coverage:all": "vitest run --coverage",
    "format:all": "pnpm -r --no-bail run format:check",
    "lint:all": "pnpm -r --no-bail run lint:check",
    "clean": "find ./ -name node_modules -type d -exec rm -rf {} +"
  },
  "keywords": [],
  "author": "Jonny Gamba",
  "license": "ISC",
  "engines": {
    "node": "22.12.0",
    "pnpm": "10.0.0"
  },
  "volta": {
    "node": "22.12.0",
    "pnpm": "10.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js"
    ]
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@pandacss/dev": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/eslint-plugin": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
